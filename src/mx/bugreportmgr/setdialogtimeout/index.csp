  pivotal: No
  name: SetDialogTimeout
  type: list
  list:
    - code: 0
      name: Do nothing
      description: This value (or the absence of this parm from the XML) will cause no change to Bug Report Manager settings; any previously selected setting will be retained.
      mx: "6.3"
      apilevel: "23"
    - code: 1
      name: False
      description: Causes the user-data collection dialog to be displayed indefinately until filled out or dismissed by the user.
      mx: "6.3"
      apilevel: "23"
    - code: 2
      name: True
