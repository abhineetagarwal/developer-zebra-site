<wap-semanticsdoc version="4.4" >

	
	<prompt name="UIProfileAction" lang="en" value="Audio UI Profile Action:" />
	<prompt name="MuteVibrateState" lang="en" value="Mute/Vibrate State:" />
	<prompt name="CurrentProfileAction" lang="en" value="Current Active Profile Action:" />

	<help name="UIProfileAction" lang="en" value="Specify the Audio UI Profile action to perform" />
	<help name="MuteVibrateState" lang="en" value="Select whether to mute the device or set the device to vibrate" />
	<help name="CurrentProfileAction" lang="en" value="Specify the Current Profile action to perform" />
		
	<list name="UIProfileAction" > 
		<item code="0" text="Do nothing" />
		<item code="1" text="Add an Audio UI Profile" />
		<item code="2" text="Remove an Audio UI Profile" />
<!--		<item code="5" text="Change an existing UI profile" /> -->
	</list>
	
	<list name="CurrentProfileAction" > 
		<item code="0" text="Do nothing" />
		<item code="1" text="Set an Audio UI Profile as the current Profile" />
		<item code="2" text="Adjust to the Preset volume level of the current Profile" />
		<item code="3" text="Set the FactoryPreset Profile as the current Profile" />
	</list>
	
	<list name="MuteVibrateState" > 
		<item code="0" text="Do not change" />
		<item code="1" text="Mute - Turn off audible sounds" />
		<item code="2" text="Vibrate - Turn off audible sounds and turn on vibrate" />
		<item code="3" text="UnMute - Turn on audible sounds" />
	</list>
	
	<list name="SetCurrentProfileOption" > 
		<item code="1" text="Do not adjust the volume level" />
		<item code="2" text="Adjust the volume level" />
	</list>			
	
	<encode code="2" name="UIProfileAction" codesize="1" method="integer" datasize="1" /> 
