	<characteristic type="AudioVolUIMgr" doc="Manage enhanced audio volume control configurations"
	>
		
		<prompt name="Version" lang="en" value="Version:" />
		<help name="Version" lang="en" value="Version of the CSP" />
		<parm name="Version" 
			datatype="String"
			promptref="Version"			
			helpref="Version"
			query="1"
			set="0"
		/>			
		
		<parm name="MuteVibrateState"
			promptref="MuteVibrateState"
			default="[.code]='0'"
			helpref="MuteVibrateState"
			listref="MuteVibrateState"
			encoderef="MuteVibrateState"
			skipif="[.code]=='0'"
			doc="Supported actions are:"
		/>
		
			
		<parm name="CurrentProfileAction"
			promptref="CurrentProfileAction"
			default="[.code]='0'"
			helpref="CurrentProfileAction"
			listref="CurrentProfileAction"
			encoderef="CurrentProfileAction"
			skipif="[.code]=='0'"
			doc="Supported actions are:"
		/>
		
		<prompt name="CurrentUIProfile" lang="en" value="Current Profile:"/>
		<help name="CurrentUIProfile" lang="en" value="Get name of the current active Profile"/>		
		<characteristic type ="CurrentUIProfile" presentif="[CurrentProfileAction.code] in (1)" doc="Supply details about an Audio UI Profile"
			query="3" promptref="CurrentUIProfile" helpref="CurrentUIProfile"
		>
			<prompt name="CurrentProfileName" lang="en" value="Name of the Current Audio UI Profile:" />
			<help name="CurrentProfileName" lang="en" value="Provide the name of the Current Audio UI Profile" />
			<parm name="CurrentProfileName" 
				datatype="string"
				value="[.text]"
				minsize="1"
				maxsize="255"
				promptref="CurrentProfileName"
				helpref="CurrentProfileName"
				encoderef="CurrentProfileName"
				query="2"
				doc="String length must be:"
			/>
			
			<prompt name="SetCurrentProfileOption" lang="en" value="Adjust current audio volume?" />
			<help name="SetCurrentProfileOption" lang="en" value="Select whether to adjust the current audio volume or not" />
			<parm name="SetCurrentProfileOption" 
				presentif="[CurrentProfileAction.code] in (1) "
				promptref="SetCurrentProfileOption"
				default="[.code]='1'"
				helpref="SetCurrentProfileOption"
				listref="SetCurrentProfileOption"
				encoderef="SetCurrentProfileOption"
			/>
		
		</characteristic>
		
		<parm name="UIProfileAction"
			promptref="UIProfileAction"
			default="[.code]='0'"
			helpref="UIProfileAction"
			listref="UIProfileAction"
			encoderef="UIProfileAction"
			skipif="[.code]=='0'"
			doc="Supported actions are:"
		/>
		
		<characteristic type ="UIProfile" presentif="[UIProfileAction.code] not in (0)" doc="Supply details about an Audio UI Profile"
		>
			<prompt name="ProfileName" lang="en" value="Name of the Audio UI Profile:" />
			<help name="ProfileName" lang="en" value="Provide the name of the Audio UI Profile definition" />
			<parm name="ProfileName"
				presentif="[UIProfileAction.code] in (1,2)"
				datatype="string"
				value="[.text]"
				minsize="1"
				maxsize="255"
				promptref="ProfileName"
				helpref="ProfileName"
				encoderef="ProfileName"
				query="2"
				doc="String length must be:"
			/>
			
		<characteristic type ="UIProfile-streamconfig" presentif="[UIProfileAction.code] in (1)" doc="Supply stream configuration details about an Audio UI Profile"
			query="2" 
		>	
		<!-- ************************ STREAM_MUSIC *************************** -->
			<prompt name="STREAM_MUSIC_Label" lang="en" value="UI Label of STREAM_MUSIC:" />
			<help name="STREAM_MUSIC_Label" lang="en" value="Provide the UI label for the music stream" />
			<parm name="STREAM_MUSIC_Label" 
				datatype="string"
				value="[.text]"
				minsize="0"
				maxsize="255"
				promptref="STREAM_MUSIC_Label"
				helpref="STREAM_MUSIC_Label"
				encoderef="STREAM_MUSIC_Label"
				query="2"
				skipif="[.text]==''"
				doc="String length must be:"
			/>
			
			<prompt name="STREAM_MUSIC_Icon" lang="en" value="UI Icon of STREAM_MUSIC:" />
			<help name="STREAM_MUSIC_Icon" lang="en" value="Provide the URL to the icon image for the music stream" />
			<parm name="STREAM_MUSIC_Icon" 
				datatype="string"
				value="[.text]"
				minsize="0"
				maxsize="255"
				promptref="STREAM_MUSIC_Icon"
				helpref="STREAM_MUSIC_Icon"
				encoderef="STREAM_MUSIC_Icon"
				query="2"
				skipif="[.text]==''"
				doc="String length must be:"
			/>
			
			<prompt name="STREAM_MUSIC_Ctrl" lang="en" value="Control Parameters of STREAM_MUSIC:" />
			<help name="STREAM_MUSIC_Ctrl" lang="en" value="Specify whether the stream is visible (1) or not (0) to the user" />
			<parm name="STREAM_MUSIC_Ctrl" 
				datatype="string"
				value="[.text]"
				minsize="0"
				maxsize="255"
				promptref="STREAM_MUSIC_Ctrl"
				helpref="STREAM_MUSIC_Ctrl"
				encoderef="STREAM_MUSIC_Ctrl"
				query="2"
				skipif="[.text]==''"
				doc="String length must be:"
			/>
			
		<!-- MODE Speaker -->
			<prompt name="STREAM_MUSIC_SPK_LEVEL" lang="en" value="Minimum, Maximum and Preset Volume Level for Speaker Mode:" />
			<help name="STREAM_MUSIC_SPK_LEVEL" lang="en" value="Specify the volume levels for music stream in speaker mode (in the order of &quot;min, max, preset&quot; separated by comma and as positive integers less than 256, e.g. &quot;10, 1, 5&quot;)" />
			<parm name="STREAM_MUSIC_SPK_LEVEL"
				datatype="string"
				promptref="STREAM_MUSIC_SPK_LEVEL"
				helpref="STREAM_MUSIC_SPK_LEVEL"
				encoderef="STREAM_MUSIC_SPK_LEVEL"
				query="2"
				skipif="[.text]==''"
				doc=":"
			/>
		<!-- MODE Receiver -->
			<prompt name="STREAM_MUSIC_RCVR_LEVEL" lang="en" value="Minimum, Maximum and Preset Volume Level for Receiver Mode:" />
			<help name="STREAM_MUSIC_RCVR_LEVEL" lang="en" value="Specify the volume levels for music stream in receiver mode (in the order of &quot;min, max, preset&quot; separated by comma and as positive integers less than 256, e.g. &quot;10, 1, 5&quot;)" />
			<parm name="STREAM_MUSIC_RCVR_LEVEL"
				datatype="string"
				promptref="STREAM_MUSIC_RCVR_LEVEL"
				helpref="STREAM_MUSIC_RCVR_LEVEL"
				encoderef="STREAM_MUSIC_RCVR_LEVEL"
				query="2"
				skipif="[.text]==''"
				doc=":"
			/>
		<!-- MODE Wired headset -->
			<prompt name="STREAM_MUSIC_WHS_LEVEL" lang="en" value="Minimum, Maximum and Preset Volume Level for Wired Headset Mode:" />
			<help name="STREAM_MUSIC_WHS_LEVEL" lang="en" value="Specify the volume levels for music stream in Wired Headset mode (in the order of &quot;min, max, preset&quot; separated by comma and as positive integers less than 256, e.g. &quot;10, 1, 5&quot;)" />
			<parm name="STREAM_MUSIC_WHS_LEVEL"
				datatype="string"
				promptref="STREAM_MUSIC_WHS_LEVEL"
				helpref="STREAM_MUSIC_WHS_LEVEL"
				encoderef="STREAM_MUSIC_WHS_LEVEL"
				query="2"
				skipif="[.text]==''"
				doc=":"
			/>
		<!-- MODE BT headset -->
			<prompt name="STREAM_MUSIC_BTHS_LEVEL" lang="en" value="Minimum, Maximum and Preset Volume Level for Bluetooth Headset Mode:" />
			<help name="STREAM_MUSIC_BTHS_LEVEL" lang="en" value="Specify the volume levels for music stream in Bluetooth Headset mode (in the order of &quot;min, max, preset&quot; separated by comma and as positive integers less than 256, e.g. &quot;10, 1, 5&quot;)" />
			<parm name="STREAM_MUSIC_BTHS_LEVEL"
				datatype="string"
				promptref="STREAM_MUSIC_BTHS_LEVEL"
				helpref="STREAM_MUSIC_BTHS_LEVEL"
				encoderef="STREAM_MUSIC_BTHS_LEVEL"
				query="2"
				skipif="[.text]==''"
				doc=":"
			/>
			
		<!-- ************************ STREAM_RING *************************** -->
