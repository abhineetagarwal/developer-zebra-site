				query="2"
				skipif="[.text]==''"
				doc=":"
			/>	
			
		<!-- ************************ STREAM_NOTIFICATION *************************** -->
			<prompt name="STREAM_NOTIFICATION_Label" lang="en" value="UI Label of STREAM_NOTIFICATION:" />
			<help name="STREAM_NOTIFICATION_Label" lang="en" value="Provide the UI label for the NOTIFICATION stream" />
