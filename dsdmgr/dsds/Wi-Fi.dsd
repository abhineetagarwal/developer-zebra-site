                        promptref ="Encryption"
                        listref ="EncryptionWEP"
                        helpref="Encryption"
                        encoderef="EncryptionWEP"
                        skipif="[.code]=='0'"
                        doc="Supported encryption types are:"
                    />

                </characteristic>

                <characteristic type="user-credentials" doc="User Credentials"
                    presentif="[NetworkAction.code] in (1) and [SecurityMode.code] in (2) and ( ( [TargetOS.code] in (1) and [UseUserProfile.code] in (0) ) or [TargetOS.code] in (2,3) ) "
                    indent="1"
                >
                    <parm name="Identity"
                        presentif="[Authentication.code] in (1:14)"
                        datatype ="String"
                        minsize="0"
                        maxsize="64"
                        promptref="Identity"
                        encoderef="Identity"
                        skipif="[.text]==''"
                        helpref="Identity"
                        doc="String length must be:"
                    />

                    <parm name="AnonymousIdentity"
                        presentif="[Authentication.code] not in (1,4)"
                        datatype ="String"
                        minsize="1"
                        maxsize="64"
                        promptref="AnonymousIdentity"
                        encoderef="AnonymousIdentity"
                        skipif="[.text]==''"
                        helpref="AnonymousIdentity"
                        doc="String length must be:"
                    />

                    <parm name="ProtectPassword"
                        presentif ="[Authentication.code] not in (1,3,12)"
                        datatype ="Boolean"
                        promptref="ProtectPassword"
                        helpref="ProtectPassword"
                        encoderef="ProtectPassword"
                    />

                    <parm name="PasswordClear"
                        presentif="[Authentication.code] not in (1,3,12) and [ProtectPassword.code] in (0)"
                        datatype ="String"
                        minsize="1"
                        maxsize="64"
                        promptref="Password"
                        encoderef="PasswordClear"
                        encrypt="0"
                        doc="String length must be:"
                    />

                    <parm name="PasswordEncrypted"
                        presentif="[Authentication.code] not in (1,3,12) and [ProtectPassword.code] in (1)"
                        datatype ="String"
                        minsize="1"
                        maxsize="64"
                        promptref="Password"
                        encoderef="PasswordEncrypted"
                        encrypt="1"
                        doc="String length must be:"
                    />

                </characteristic>

                <parm name="OptionalServerCertificate"
                    presentif="[Authentication.code] in (1,2,3,5,6,7,8,9,10,11,13,14)"
                    datatype ="String"
                    default="[.text]=''"
                    minsize="0"
                    maxsize="64"
                    promptref="OptionalServerCertificate"
                    encoderef="OptionalServerCertificate"
                    helpref="OptionalServerCertificate"
                    doc="String length must be:"
                    skipif="[.text]==''"
                />

                <parm name="MandatoryServerCertificate"
                    presentif="[Authentication.code] in (12)"
                    datatype ="String"
                    minsize="0"
                    maxsize="64"
                    promptref="MandatoryServerCertificate"
                    encoderef="MandatoryServerCertificate"
                    helpref="MandatoryServerCertificate"
                    doc="String length must be:"
                />

                <parm name="MandatoryClientCertificate"
                    presentif="[Authentication.code] in (1,3,12)"
                    datatype ="String"
                    minsize="1"
                    maxsize="64"
                    promptref="MandatoryClientCertificate"
                    encoderef="MandatoryClientCertificate"
                    helpref="MandatoryClientCertificate"
                    doc="String length must be:"
                />

                <parm name="OptionalClientCertificate"
                    presentif="[Authentication.code] in (2,5,6,7,8,9,10,11,13,14)"
                    datatype ="String"
                    default="[.text]=''"
                    minsize="0"
                    maxsize="64"
                    promptref="OptionalClientCertificate"
                    encoderef="OptionalClientCertificate"
                    helpref="OptionalClientCertificate"
                    doc="String length must be:"
                    skipif="[.text]==''"
                />

            </characteristic>

            <characteristic type="key-details" doc="Encryption Key details"
                presentif="[NetworkAction.code] in (1) and [SecurityMode.code] in (1)"
                indent="1"
            >

                <parm name="KeyType"
                    promptref="KeyType"
                    default="[.code]='2'"
                    value="[.text]"
                    helpref="KeyType"
                    listref="KeyType"
                    encoderef="KeyType"
                    doc="Supported selections are:"
                />

                <parm name="ProtectKey"
                    datatype ="Boolean"
                    promptref="ProtectKey"
                    helpref="ProtectKey"
                    encoderef="ProtectKey"
                />

                <parm name="HexKeyClear"
                    presentif ="[KeyType.code] in (1) and [WPAMode.code] not in (4) and [ProtectKey.code] in (0)"
                    datatype ="String"
                    minsize="64"
                    maxsize="64"
                    dynamic ="[?]"
                    promptref="HexKey"
                    helpref="HexKey"
                    encoderef="HexKeyClear"
                    encrypt="0"
                    doc="String length must be:"
                />

                <parm name="HexKeyEncrypted"
                    presentif ="[KeyType.code] in (1) and [WPAMode.code] not in (4) and [ProtectKey.code] in (1)"
                    datatype ="String"
                    minsize="64"
                    maxsize="64"
                    dynamic ="[?]"
                    promptref="HexKey"
                    helpref="HexKey"
                    encoderef="HexKeyEncrypted"
                    encrypt="1"
                    doc="String length must be:"
                />

                <parm name="PassphraseWEPClear"
                    presentif ="[KeyType.code] in (2) and [WPAMode.code] in (4) and [ProtectKey.code] in (0)"
                    datatype ="String"
                    minsize="4"
                    maxsize="32"
                    dynamic ="[?]"
                    promptref="Passphrase"
                    helpref="Passphrase"
                    encoderef="PassphraseWEPClear"
                    encrypt="0"
                    doc="String length must be:"
                />

                <parm name="PassphraseWEPEncrypted"
                    presentif ="[KeyType.code] in (2) and [WPAMode.code] in (4) and [ProtectKey.code] in (1)"
                    datatype ="String"
                    minsize="4"
                    maxsize="32"
                    dynamic ="[?]"
                    promptref="Passphrase"
                    helpref="Passphrase"
                    encoderef="PassphraseWEPEncrypted"
                    encrypt="1"
                    doc="String length must be:"
                />

                <parm name="PassphraseWPAClear"
                    presentif ="[KeyType.code] in (2) and [WPAMode.code] not in (4) and [ProtectKey.code] in (0)"
                    datatype ="String"
                    minsize="8"
                    maxsize="63"
                    dynamic ="[?]"
                    promptref="Passphrase"
                    helpref="Passphrase"
                    encoderef="PassphraseWPAClear"
                    encrypt="0"
                    doc="String length must be:"
                />

                <parm name="PassphraseWPAEncrypted"
                    presentif ="[KeyType.code] in (2) and [WPAMode.code] not in (4) and [ProtectKey.code] in (1)"
                    datatype ="String"
                    minsize="8"
                    maxsize="63"
                    dynamic ="[?]"
                    promptref="Passphrase"
                    helpref="Passphrase"
                    encoderef="PassphraseWPAEncrypted"
                    encrypt="1"
                    doc="String length must be:"
                />

                <parm name="HexKeyWep40Clear"
                    presentif="[KeyType.code] in (1) and [WPAMode.code] in (4) and [EncryptionWEP.code] in (1) and [ProtectKey.code] in (0)"
                    datatype="String"
                    minsize ="10"
                    maxsize ="10"
                    promptref="HexKey"
                    helpref="HexKeyWep40"
                    encoderef="HexKeyWep40Clear"
                    encrypt="0"
                    doc="String length must be:"
                />

                <parm name="HexKeyWep40Encrypted"
                    presentif="[KeyType.code] in (1) and [WPAMode.code] in (4) and [EncryptionWEP.code] in (1) and [ProtectKey.code] in (1)"
                    datatype="String"
                    minsize ="10"
                    maxsize ="10"
                    promptref="HexKey"
                    helpref="HexKeyWep40"
                    encoderef="HexKeyWep40Encrypted"
                    encrypt="1"
                    doc="String length must be:"
                />

                <parm name="HexKeyWep104Clear"
                    presentif="[KeyType.code] in (1) and [WPAMode.code] in (4) and [EncryptionWEP.code] in (2) and [ProtectKey.code] in (0)"
                    datatype="String"
                    minsize ="26"
                    maxsize ="26"
                    promptref="HexKey"
                    helpref="HexKeyWep104"
                    encoderef="HexKeyWep104Clear"
                    encrypt="0"
                    doc="String length must be:"
                />

                <parm name="HexKeyWep104Encrypted"
                    presentif="[KeyType.code] in (1) and [WPAMode.code] in (4) and [EncryptionWEP.code] in (2) and [ProtectKey.code] in (1)"
                    datatype="String"
                    minsize ="26"
                    maxsize ="26"
                    promptref="HexKey"
                    helpref="HexKeyWep104"
                    encoderef="HexKeyWep104Encrypted"
                    encrypt="1"
                    doc="String length must be:"
                />

                <parm name="WepKeyIndex"
                    presentif="[WPAMode.code] in (4)"
                    promptref="WepKeyIndex"
                    helpref="WepKeyIndex"
                    encoderef="WepKeyIndex"
                    listref="WepKeyIndex"
                    doc="Supported selections are:"
                />

            </characteristic>

            <parm name="UseDHCP"
                presentif="[NetworkAction.code] in (1)"
                datatype="Boolean"
                default="[.code]='1'"
                promptref="UseDHCP"
                helpref="UseDHCP"
                encoderef="UseDHCP"
                doc="Supported selections are:"
            />

            <characteristic type="ip-details" doc="Static IP address details"
                presentif="[NetworkAction.code] in (1) and [UseDHCP.code] in (0)"
                indent="1"
            >

                <parm name="IpAddress"
                    datatype="IPV4"
                    default="[.text]='127.0.0.1'"
                    promptref="IpAddress"
                    helpref="IpAddress"
                    encoderef="IpAddress"
                    doc="Must be a valid IPV4 address, example: 191.168.0.1"
                />

                <parm name="IpGateway1"
                    datatype="IPV4"
                    default="[.text]='127.0.0.1'"
                    promptref="IpGateway1"
                    helpref="IpGateway1"
                    encoderef="IpGateway1"
                    doc="Must be a valid IPV4 address, example: 191.168.0.1"
                />

                <parm name="IpGateway2"
                    datatype="IPV4"
                    default="[.text]='127.0.0.1'"
                    promptref="IpGateway2"
                    helpref="IpGateway2"
                    encoderef="IpGateway2"
                    presentif="[TargetOS.code] in (1) and [NetworkAction.code] in (1)"
                    doc="Must be a valid IPV4 address, example: 191.168.0.1"
                />

                <parm name="IpMask"
                    datatype="IPV4"
                    default="[.text]='255.255.255.0'"
                    promptref="IpMask"
                    helpref="IpMask"
                    encoderef="IpMask"
                    doc="Must be a valid IPV4 subnet mask, example: 255.255.255.0"
                />

                <parm name="IpDns1"
                    datatype="IPV4"
                    default="[.text]='127.0.0.1'"
                    promptref="IpDns1"
                    helpref="IpDns1"
                    encoderef="IpDns1"
                    doc="Must be a valid IPV4 address, example: 191.168.0.1"
                />

                <parm name="IpDns2"
                    datatype="IPV4"
                    default="[.text]=''"
                    promptref="IpDns2"
                    helpref="IpDns2"
                    encoderef="IpDns2"
                    skipif="[.text]==''"
                    doc="Must be a valid IPV4 address, example: 191.168.0.1"
                />

            </characteristic>

            <parm name="UseProxy"
                presentif="[NetworkAction.code] in (1)"
                datatype="Integer"
                default="[.code]='0'"
                promptref="UseProxy"
                helpref="UseProxy"
                listref="UseProxy"
                encoderef="UseProxy"
                doc="Supported values are:"
            />


            <characteristic type="proxy-details" doc="Proxy Server details"
                presentif="[NetworkAction.code] in (1)"
                indent="1"
            >

                <parm name="ProxyHostName"
                    presentif="[UseProxy.code] in (1)"
                    promptref="ProxyHostName"
                    datatype="String"
                    minsize ="1"
                    maxsize ="64"
                    helpref="ProxyHostName"
                    encoderef="ProxyHostName"
                    doc="String length must be:"
                />

                <parm name="ProxyPort"
                    presentif="[UseProxy.code] in (1)"
                    promptref="ProxyPort"
                    datatype="Integer"
                    helpref="ProxyPort"
                    encoderef="ProxyPort"
