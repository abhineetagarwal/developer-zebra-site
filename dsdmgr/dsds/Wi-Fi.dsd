                <parm name="BypassProxy"
                    presentif="[UseProxy.code] in (1)"
                    promptref="BypassProxy"
                    default="[.text]=''"
                    datatype="String"
                    maxsize ="256"
                    helpref="BypassProxy"
                    encoderef="BypassProxy"
                    skipif="[.text]==''"
                    doc="String length must be:"
                />
                <parm name="PROXYPAC"
                    presentif="[UseProxy.code] in (3)"
                    promptref="ProxyPAC"
                    datatype="String"
                    maxsize ="256"
                    helpref="ProxyPAC"
                    encoderef="ProxyPAC"
                    doc="String length must be:"
                />

            </characteristic>

        </characteristic>

    </characteristic>

</wap-semanticsdoc>

