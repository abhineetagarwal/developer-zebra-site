        />


        <parm name="IntegerConditionTest"
            promptref="IntegerConditionTest"
            helpref="IntegerConditionTest"
            listref="IntegerConditionTest"
            encoderef="IntegerConditionTest"
            datatype="integer"
            doc="Supported Tests are:"
        />

