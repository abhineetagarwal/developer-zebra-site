            promptref="IntegerConstantValue"
            helpref="IntegerConstantValue"
            encoderef="IntegerConstantValue"
            datatype="integer"
            mininteger="-32768"
            maxinteger="32767"
            doc="Support values must be:"
        />

        <parm name="ArgStorageSpace"
            presentif="[IntegerSourceType.code] in (2) and [IntegerSystemValue.code] in (5)"
            mininteger="1"
            maxinteger="1024"
            promptref="ArgStorageSpace"
            helpref="ArgStorageSpace"
            encoderef="ArgStorageSpace"
            datatype="integer"
            doc="Value must be a valid integer"
        />


    </characteristic>

    <characteristic type="StringDetails" presentif="[DataType.code] in (3)" doc="Details for String Data Type">

