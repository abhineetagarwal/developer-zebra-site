            encoderef="IntegerCustomMember"
            datatype="string"
            value="[.text]"
            minsize="1"
            doc="Member name length must be:"
        />

        <parm name="IntegerCustomArgs"
            presentif="[IntegerSourceType.code] in (4)"
            promptref="IntegerCustomArgs"
            helpref="IntegerCustomArgs"
            encoderef="IntegerCustomArgs"
            datatype="string"
            value="[.text]"
            doc="Leave empty if no arguments"
        />

        <parm name="ArgStorageSpaceDir"
            presentif="[IntegerSourceType.code] in (2) and [IntegerSystemValue.code] in (5)"
            promptref="ArgStorageSpaceDir"
            helpref="ArgStorageSpaceDir"
            listref="ArgStorageSpaceDir"
            encoderef="ArgStorageSpaceDir"
            datatype="integer"
