<characteristic type="ConditionMgr" doc="Manage Testing of Device Conditions">

    <parm name="DataType"
        promptref="DataType"
        helpref="DataType"
        listref="DataType"
        encoderef="DataType"
        datatype="integer"
        doc="Supported Types are:"
    />

    <characteristic type="BooleanDetails" presentif="[DataType.code] in (1)" doc="Details for Boolean Data Type">

        <parm name="BooleanSourceType"
            promptref="BooleanSourceType"
            helpref="BooleanSourceType"
            listref="BooleanSourceType"
            encoderef="BooleanSourceType"
            datatype="integer"
            doc="Supported Types are:"
        />

        <parm name="BooleanSystemProperty"
            presentif="[BooleanSourceType.code] in (1)"
            promptref="BooleanSystemProperty"
            helpref="BooleanSystemProperty"
            listref="BooleanSystemProperty"
            encoderef="BooleanSystemProperty"
            datatype="integer"
            value="[.code]"
            doc="Supported Properties are:"
        />

        <parm name="BooleanSystemValue"
            presentif="[BooleanSourceType.code] in (2)"
            promptref="BooleanSystemValue"
            helpref="BooleanSystemValue"
            listref="BooleanSystemValue"
            encoderef="BooleanSystemValue"
            datatype="integer"
            value="[.code]"
            doc="Supported System Values are:"
        />

        <parm name="BooleanSystemSetting"
            presentif="[BooleanSourceType.code] in (3)"
            promptref="BooleanSystemSetting"
            helpref="BooleanSystemSetting"
            encoderef="BooleanSystemSetting"
            datatype="string"
            value="[.text]"
            minsize="1"
            doc="System Setting name length must be:"
        />

        <parm name="BooleanCustomClass"
            presentif="[BooleanSourceType.code] in (4)"
            promptref="BooleanCustomClass"
            helpref="BooleanCustomClass"
            encoderef="BooleanCustomClass"
            datatype="string"
            value="[.text]"
            minsize="1"
            doc="Class name length must be:"
        />

        <parm name="BooleanCustomMember"
            presentif="[BooleanSourceType.code] in (4)"
            promptref="BooleanCustomMember"
            helpref="BooleanCustomMember"
            encoderef="BooleanCustomMember"
            datatype="string"
            value="[.text]"
            minsize="1"
            doc="Member name length must be:"
        />

        <parm name="BooleanCustomArgs"
            presentif="[BooleanSourceType.code] in (4)"
            promptref="BooleanCustomArgs"
            helpref="BooleanCustomArgs"
            encoderef="BooleanCustomArgs"
            datatype="string"
            value="[.text]"
            doc="Leave empty if no arguments"
        />

        <parm name="BooleanSystemFunction"
            presentif="[BooleanSourceType.code] in (5)"
            promptref="BooleanSystemFunction"
            helpref="BooleanSystemFunction"
            listref="BooleanSystemFunction"
            encoderef="BooleanSystemFunction"
            datatype="integer"
            value="[.code]"
            doc="Supported System Functions are:"
        />

        <parm name="ArgFilePathAndName"
            presentif="[BooleanSourceType.code] in (5) and [BooleanSystemFunction.code] in (1)"
            promptref="ArgFilePathAndName"
            helpref="ArgFilePathAndName"
            encoderef="ArgFilePathAndName"
            datatype="string"
            value="[.text]"
            minsize="1"
            doc="String length must be:"
        />

        <parm name="ArgIpAddress"
            presentif="[BooleanSourceType.code] in (5) and [BooleanSystemFunction.code] in (2)"
            promptref="ArgIpAddress"
            helpref="ArgIpAddress"
            encoderef="ArgIpAddress"
            datatype="IPV4"
            doc="Value must be a valid IPV4 Address"
        />

        <!-- Not required for Stagenow 2.1 -->
        <!--<parm name="BooleanConditionTest"
            promptref="BooleanConditionTest"
            helpref="BooleanConditionTest"
            listref="BooleanConditionTest"
            encoderef="BooleanConditionTest"
            datatype="integer"
            doc="Supported Tests are:"
        />-->

    </characteristic>

    <characteristic type="IntegerDetails" presentif="[DataType.code] in (2)" doc="Details for Integer Data Type">

        <parm name="IntegerSourceType"
            promptref="IntegerSourceType"
            helpref="IntegerSourceType"
