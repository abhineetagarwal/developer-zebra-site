	<characteristic  type="Intent"
		doc="Send an intent to an activity or service"
	>

		<parm name="Action"
			promptref="Action"
			default="[.code]='1'"
			value="[.text]"
			helpref="Action"
			listref="Action"
			encoderef="Action"
			doc="Supported actions are:"
		/>

		<parm name="ActionName"
			datatype="String"
			minsize="1"
			maxsize="255"
			promptref="ActionName"
			helpref="ActionName"
			encoderef="ActionName"
			doc="String length must be:"
		/>

		<parm name="Type"
			datatype="String"
			minsize="0"
			maxsize="255"
			promptref="Type"
			helpref="Type"
			encoderef="Type"
			skipif="[.text]==''"
			doc="String length must be:"
		/>

		<parm name="Package"
			presentif="[Action.code] in (1,2)"
			datatype="String"
			minsize="0"
			maxsize="255"
			promptref="Package"
			helpref="Package"
			encoderef="Package"
			skipif="[.text]==''"
			doc="String length must be:"
		/>

		<parm name="Class"
			presentif="[Action.code] in (1,2)"
			datatype="String"
			minsize="0"
			maxsize="255"
			promptref="Class"
			helpref="Class"
			encoderef="Class"
			skipif="[.text]==''"
			doc="String length must be:"
		/>
		
		<parm name="Uri"
			presentif="[Action.code] in (1,2)"
			datatype="String"
			minsize="0"
			maxsize="255"
			promptref="Uri"
			helpref="Uri"
			encoderef="Uri"
			skipif="[.text]==''"
			doc="String length must be:"
		/>

		<parm name="File"
			presentif="[Action.code] in (1,2)"
			datatype="String"
			minsize="0"
			maxsize="255"
			promptref="File"
			helpref="File"
			encoderef="File"
			skipif="[.text]==''"
			doc="String length must be:"
		/>

		<characteristic  type="Extra"
			doc="Specify extra data to attach to the intent"
		>

			<parm name="ExtraType"
				datatype="String"
				promptref="ExtraType"
				helpref="ExtraType"
				listref="ExtraType"
				encoderef="ExtraType"
				skipif="[.code]=='none'"
				doc="Supported options are:"
			/>

			<parm name="ExtraName"
				presentif="[ExtraType.code]!='none'"
				datatype="String"
				minsize="1"
				maxsize="255"
				promptref="ExtraName"
				helpref="ExtraName"
				encoderef="ExtraName"
				skipif="[.text]==''"
				doc="String length must be:"
			/>

			<parm name="ExtraValue"
				presentif="[ExtraType.code]!='none'"
				datatype="String"
				minsize="1"
				maxsize="255"
				promptref="ExtraValue"
				helpref="ExtraValue"
				encoderef="ExtraValue"
				skipif="[.text]==''"
				doc="String length must be:"
			/>

		</characteristic>

		<characteristic  type="Extra1"
			doc="Specify extra data to attach to the intent"
		>

			<parm name="Extra1Type"
				datatype="String"
				promptref="Extra1Type"
				helpref="Extra1Type"
				listref="ExtraType"
